<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<s:DataGrid id="dg" x="0" y="0" width="100%" height="100%" alpha="0.5" color="#000000"
				fontSize="15">
		<s:columns>
			<s:ArrayList>
				<s:GridColumn dataField="playerName" headerText="ID"></s:GridColumn>
				<s:GridColumn dataField="roomNo" headerText="房间编号"></s:GridColumn>
				<s:GridColumn dataField="preMoney" headerText="番数"></s:GridColumn>
				<s:GridColumn dataField="winLossMoneyAfterTax" headerText="入局点数"></s:GridColumn>
				<s:GridColumn dataField="afterMoney" headerText="创建时间"></s:GridColumn>
				<s:GridColumn dataField="gameTime" headerText="是否加密"></s:GridColumn>
				<s:GridColumn headerText="操作">
					<s:itemRenderer>
						<fx:Component>
							<s:GridItemRenderer>
	
								
								<s:HGroup>

								</s:HGroup>
							</s:GridItemRenderer>
						</fx:Component>
					</s:itemRenderer>
				</s:GridColumn>
			</s:ArrayList>
		</s:columns>
	</s:DataGrid>
</s:View>
