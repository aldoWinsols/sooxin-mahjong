<?xml version="1.0" encoding="utf-8"?>
<s:BorderContainer xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:view="com.view.*"
				   width="960" height="640" backgroundAlpha="0"
				   borderVisible="false" cornerRadius="10"
				   creationComplete="bordercontainer2_creationCompleteHandler(event)">
		
	<fx:Script>
		<![CDATA[
			import com.control.LianwangHomeControl;
			import com.services.MainPlayerService;
			
			import mx.events.FlexEvent;
			
			import spark.events.ElementExistenceEvent;
			

			
			protected function bordercontainer2_creationCompleteHandler(event:FlexEvent):void
			{
				// TODO Auto-generated method stub
				new LianwangHomeControl(this);
			}
			
		]]>
	</fx:Script>
	
	<s:states>
		<s:State name="main"/>
		<s:State name="log"/>
	</s:states>
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	
	<view:Log includeIn="log" x="0" y="-1">
	</view:Log>
	<s:TabbedViewNavigator includeIn="main" x="0" y="90" width="960" height="460"
						   accentColor="#C7C7C7" alpha="0.7" backgroundAlpha="0.3" fontSize="30">
		<s:ViewNavigator width="100%" height="100%" label="休闲房间" firstView="views.View1"
						 icon="assets/main/54.png" title="休闲房间"/>
		<s:ViewNavigator width="100%" height="100%" label="附近房间" firstView="views.View2"
						 icon="assets/main/81.png" title="附近房间"/>
		<s:ViewNavigator width="100%" height="100%" label="私人房间" firstView="views.View3"
						 icon="assets/main/49.png" title="私人房间"/>
	</s:TabbedViewNavigator>
	<s:Image includeIn="main" x="782" y="10" source="assets/main/liwuB.png"/>
	<s:Image includeIn="main" x="880" y="10" source="assets/main/chongzhiB.png"/>
	<s:Image includeIn="main" id="backB" x="880" y="560" source="assets/main/backB.png"/>
</s:BorderContainer>
