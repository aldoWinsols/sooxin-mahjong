<?xml version="1.0" encoding="utf-8"?>
<spark:DataGridSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" xmlns:spark="spark.skins.spark.*" width="400" height="300">
	<fx:Script>
		<![CDATA[
			import spark.primitives.Rect;
			
			override public function set background(value:Rect):void
			{
				// TODO Auto Generated method stub
				super.background = aa;
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		</fx:Declarations>
		
	<s:Rect id="aa" left="0" right="0" top="0" bottom="0" alpha="0">
		<s:stroke>
			<!--- @private -->
			<s:SolidColorStroke id="borderStroke" weight="1"/>
		</s:stroke>
	</s:Rect>    
	
</spark:DataGridSkin>
