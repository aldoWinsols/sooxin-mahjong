		 xmlns:view="com.stock.view.*" creationComplete="group1_creationCompleteHandler(event)"
		 width="100%" height="100%">
	<fx:Script>
		<![CDATA[
			import com.stock.control.NowWeituoControl;
			
			import mx.events.FlexEvent;
			
