<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 width="100" height="30" creationComplete="group1_creationCompleteHandler(event)">
	<fx:Script>
		<![CDATA[
			import com.missile.control.MainSenceControl;
			
			import mx.events.FlexEvent;
			
			public var mName:String;
			
			protected function group1_creationCompleteHandler(event:FlexEvent):void
			{
				// TODO Auto-generated method stub
				this.addEventListener(MouseEvent.CLICK,clickHandler);
			}
			
			private function clickHandler(e:MouseEvent):void{
				MainSenceControl.instance.changeState(this.mName);
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<s:BorderContainer width="100" height="30" backgroundColor="#000000">
		
	</s:BorderContainer>
</s:Group>
